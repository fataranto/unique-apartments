    <p>-Este es el dashboard del usuario, aquí tendrá la posibilidad de actualizar sus datos y todo lo referente a la
      gestión de los pisos que haya resrvado.</p>
    <p> username: <%=user.username%> </p>
    <p> Name: <%=user.name%> </p>
    <p> Last Name: <%=user.lastname%> </p>
    <p> Email: <%=user.email%> </p>
    <p> <% if(user.roles[1]){ %>
      <%=user.roles[1].name%>
      <% }   %> </p>

  </section>

  <!-- Si hay apartamentos los listo en una tabla con un boton que enlace a la edición del apartamento y otro para elimarlo de la bbdd-->

  <% if(apartments){ %>
  <section class="container">
    <h3>My apartaments</h3>

 

    <table id="myApartments" class="table table-hover text-nowrap table-striped">
      <thead>
        <tr>
          <th>Title</th>
          <th>State</th>
          <th>City</th>
          <th>Edit / Delete</th>
        </tr>
      </thead>
      <tbody>
        <% apartments.forEach(function(apartment){ %>

        <tr>

          <td><a href="/apartment/<%=apartment._id%>"><%=apartment.title%></a></td>
          <td><%=apartment.location.state%></td>
          <td><%=apartment.location.city%></td>
          <td>
            <a href="/apartment/<%=apartment._id%>/edit" class="btn btn-primary">Edit</a>
            <a href="/apartment/<%=apartment._id%>/delete" class="btn btn-danger">Delete</a>
          </td>
        </tr>

        <% }); %>
      </tbody>
    </table>

  </section>
  <% } %>







